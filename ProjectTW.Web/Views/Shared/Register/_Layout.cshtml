@using System.Web.Optimization;

<!DOCTYPE html>
<html class=" ">
<head>
    <meta charset="utf-8" />
    <title>EasyMed-Register</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="assets/images/favicon.ico">

    <!-- App css -->
    <link href="~/Template/aditional/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Template/aditional/assets/css/icons.min.css" rel="stylesheet" />
    <link href="~/Template/aditional/assets/css/app.min.css" rel="stylesheet" />

    @if (IsSectionDefined("Styles"))
    {
        @RenderSection("Styles", required: false)
    }
</head>
<body>
    <div class="account-pages mt-5 mb-5">
        @Html.Partial("Register/_Register")
    </div>
    @RenderBody()

    <script src="~/Template/aditional/assets/js/vendor.min.js"></script>
    <script src="~/Template/aditional/assets/js/app.min.js"></script>
</body>

</html>